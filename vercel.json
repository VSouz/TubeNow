{
    "version": 2,
    "builds": [
        {
            "src": "./ytsave.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/ytsave.py",
            "config": {
                "runtime": "python3.7.7"
            }
        }
    ]
}
